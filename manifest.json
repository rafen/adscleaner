{
    "name": "Ads cleaner!",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Let clean the ads and get a better layout!",
    "icons": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
    },
    "content_scripts": [
        {
          "matches": ["http://grooveshark.com/*",
                      "http://*.wikipedia.org/*",
                      "http://*.youtube.com/*",
                      "https://www.facebook.com/*", "http://www.facebook.com/*",
                      "http://*.mail.live.com/*", "https://*.mail.live.com/*",
                      "http://*.seriales.us/*"],
          "js": ["jquery.js", "adscleaner.js"]
        }
    ]
}
